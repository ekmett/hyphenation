ghc-head:               True
no-tests-no-benchmarks: False
unconstrained:          False
allow-failures:         <7.3
hlint:                  True
hlint-options:          --cpp-ansi --cpp-define=HLINT
irc-channels:           irc.freenode.org#haskell-lens

constraint-set embed
  constraints: hyphenation +embed
  tests:       True
  run-tests:   True
  benchmarks:  True
  haddock:     True
